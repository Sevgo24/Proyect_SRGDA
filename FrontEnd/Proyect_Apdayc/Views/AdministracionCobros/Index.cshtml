    @{
        ViewBag.Title = "Index";
    }


    <!DOCTYPE html>

    <html>
    <head>
        <meta name="viewport" content="width=device-width" />
        <meta http-equiv="cache-control" content="no-cache" />
        <title>ADMINISTRACION COBROS </title>
        <script src="@Url.Content("~/Scripts/SGRDA/generales.js")"></script>
        <script src="@Url.Content("~/Scripts/SGRDA/comun.dropdownlist.js")"></script>
        <script src="@Url.Content("~/Scripts/SGRDA/comun.autocompletar.js")"></script>
        @*<script src="@Url.Content("~/Scripts/SGRDA/comun.buscador.socio.js")"></script>*@
        <script src="@Url.Content("~/Scripts/SGRDA/Administracion/mantenimiento.administracion.cobros.js")"></script>
        <script src="@Url.Content("~/Scripts/SGRDA/comun.buscador.oficina.js")"></script>
        <script src="@Url.Content("~/Scripts/SGRDA/comun.buscador.correlativo.js")"></script>
        <script src="@Url.Content("~/Scripts/SGRDA/comun.buscador.socio.js")"></script>
        <script src="@Url.Content("~/Scripts/SGRDA/comun.buscador.socio.aux.js")"></script>

    </head>
    <body>
        <div id="ContenedormvOficina"></div>   
        <div id="ContenedormvBuscarCorrelativo"></div>
        <div id="ContenedormvBuscarSocio"></div>
        <table width='100%' border='0' cellspacing='0' cellpadding='0'>
            <tr>
                <td style=" vertical-align:bottom;">
                    <div id='divTitulo'>MODULO DE ADMINISTRACION DE COBROS</div>
                </td>
            </tr>
            <tr><td style=" vertical-align: top;"><hr></td></tr>
            <tr>
                <td align="center">
                    <table width='98%' border='0' id='FiltroTabla'>
                        <tr>
                            <td align="left" style="width:200px">CODIGO DE COBRO :</td>
                            <td><input type="text" id="txtcodcob" /></td>
                            <td align="left" style="width:200px">NUMERO DE OPERACION :</td>
                            <td><input type="text" id="txtnumope" /></td>
                            <td align="left" style="width:200px">MONTO DE DEPOSITO :</td>
                            <td><input type="text" id="txtnumontodep" /></td>
                            
                        </tr>
                        <tr>
                            <td>
                                Banco:
                            </td>
                            <td align="left" style="width:200px">
                                <select id="ddlBanco"></select>
                            </td>
                            <td align="left" style="width:200px">
                                CUENTA :
                            </td>
                            <td align="left" style="width:200px">
                                <select id="ddlCuenta"></select>
                            </td>
                            <td align="left" style="width:200px">
                                ESTADO DE CONFIRMACION
                            </td>
                            <td>
                                <select id="ddlEstadoConfirmacion"> </select> 
                            </td>
                        </tr>
                        <tr>
                            <td align="left" style="width:200px">FECHA  DE:</td>
                            <td><select id="ddlTipoFecha">
                                    <option value="0">SELECCIONE</option>
                                    <option value="1">CREACION</option>
                                    <option value="2">CANCELACION</option>
                                    <option value="3">INACTIVACION</option>
                                </select></td>
                            <td align="left" style="width:200px">FECHA  INICIAL:</td>
                            <td><input type="text" id="txtFecCreaInicial" /></td>
                            <td align="left" style="width:200px">FECHA  FINAL:</td>
                            <td><input type="text" id="txtFecCreaFinal" /></td>
                        </tr>
                        <tr>
                            <td>OFICINA RECAUDO :</td>
                            <td><input id="hidOficina" value="0" type="hidden"><img src="@Url.Content("~/Images/botones/buscar.png")" id="btnBuscaOficina" style="cursor: pointer;" alt="Búsqueda de Oficina" title="Búsqueda de Oficina"><label id="lbOficina" style="cursor: pointer;" alt="Búsqueda de Oficina" title="Búsqueda de Oficina">SELECCIONE OFICINA</label></td>
                            <td>SERIE :</td>
                            <td>
                                <img src="@Url.Content("~/Images/botones/buscar.png")" id="btnBuscarCorrelativo" style="cursor:pointer;" alt="Búsqueda de Serie" title="Búsqueda de Serie" />
                                <input type="hidden" id="hidCorrelativo" value="0" />
                                <label id="lbCorrelativo" style="cursor: pointer; font-weight:bold;" alt="Búsqueda de Serie" title="Búsqueda de Serie">
                                    Seleccione una Serie.
                                </label>
                            </td>
                            <td align="left" style="width:200px">NUMERO:</td>
                            <td><input type="text" id="txtnum"></td>
                        </tr>
                        <tr>
                            <td> SOCIO:</td>
                            <td>
                                <table border="0" style="border-collapse: collapse;">
                                    <tr>
                                        <td>
                                            <img src="@Url.Content("~/Images/botones/buscar.png")" id="btnBuscarBS" style="cursor:pointer;" alt="Búsqueda de Responsable" title="Búsqueda de Responsable" />
                                        </td>
                                        <td>
                                            <input type="hidden" id="hidResponsable" />
                                            <lable id="lbResponsable" style="cursor:pointer;" alt="Búsqueda de Responsable" title="Búsqueda de Responsable">Todos</lable>
                                        </td>
                                    </tr>
                                </table>
                            </td>
                        </tr>
                    </table>
                </td>
            </tr>
            <tr id="trCantidadCobros"><td colspan="8" align="left"  style="padding-left:30px;font-size:small;font-weight:bold">CANTIDAD DE COBROS :<label id="lblCantCobros">...</label></td></tr>
            <tr>
                <td colspan="8" align="center">
                    <button id='btnBuscarCobros' class="boton" name='btnBuscarCobros'>
                        Buscar
                        <img src="@Url.Content("~/Images/botones/buscar2.png")" width="16px" />
                    </button>
                    <button id='btnLimpiarCobros' class="boton" name='btnLimpiarCobros'>
                        Limpiar
                        <img src="@Url.Content("~/Images/botones/refresh.png")" width="16px" />
                    </button>

                    <div style="float: right;">
                        <button id='btnPdf' class="boton" name='btnPdf'>
                            Rep. PDF
                            <img src="@Url.Content("~/Images/botones/pdf.png")" width="24px" />
                        </button>
                        &nbsp;
                        <button id='btnExcel' class="boton" name='btnExcel'>
                            Rep. EXCEL
                            <img src="@Url.Content("~/Images/botones/excel.png")" width="24px" />
                        </button>
                        <img src="@Url.Content("~/Images/botones/delete.png")" alt="Inactivar datos de usuario." title="Inactivar datos de usuario." id="btnEliminarLic" />
                        &nbsp;
                    </div>

                </td>
            </tr>
            <tr>
                <td colspan="8">
                    <div id="grid"></div>
                </td>
            </tr>
            <tr>
                <td style="vertical-align:central;text-align:center">
                    <div id="contenedor" style="display:none"><iframe src="" width="100%" height="900" id="externo"></iframe></div>
                </td>
            </tr>
        </table>
    </body>
</html>





