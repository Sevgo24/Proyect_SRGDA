
@{
    ViewBag.Title = "Index";
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <meta http-equiv="cache-control" content="no-cache" />
    <title> TASA DE CAMBIO  </title>
    @*<script src="@Url.Content("~/Scripts/SGRDA/Mantenimientos/mantenimiento.establecimiento.js")"></script>*@
    <script src="@Url.Content("~/Scripts/SGRDA/generales.js")"></script>
    <script src="@Url.Content("~/Scripts/SGRDA/comun.dropdownlist.js")"></script>
    <script src="@Url.Content("~/Scripts/SGRDA/comun.autocompletar.js")"></script>
    @*<script src="@Url.Content("~/Scripts/SGRDA/comun.buscador.socio.js")"></script>*@
    <script src="@Url.Content("~/Scripts/SGRDA/Administracion/mantenimiento.administracion.tasa.cambio.js")"></script>
    @*<script src="@Url.Content("~/Scripts/SGRDA/Administracion/mantenimiento.agrupacion.licencia.division.js")"></script>*@
    @*<script src="@Url.Content("~/Scripts/SGRDA/comun.buscador.grupo.facturacion.js")"></script>*@
    @*<script src="@Url.Content("~/Scripts/SGRDA/comun.buscador.socio.aux.js")"></script>
        <script src="@Url.Content("~/Scripts/SGRDA/comun.buscador.oficina.js")"></script>*@
    @*<script src="@Url.Content("~/Scripts/SGRDA/Licenciamiento/licenciamiento.nuevo.js")"></script>*@
    @*<script src="@Url.Content("~/Scripts/SGRDA/Licenciamiento/licenciamiento.division.js")"></script>*@
</head>
<body>
    <table width='100%' border='0' cellspacing='0' cellpadding='0'>
        <tr>
            <td style=" vertical-align:bottom;">
                <div id='divTitulo'>MODULO DE ADMINISTRACION  - TASA DE CAMBIO </div>
            </td>
        </tr>
        <tr><td style=" vertical-align: top;"><hr></td></tr>
        <tr>
            <td align="center">
                <table width='98%' border='0' id='FiltroTabla'>
                    <tr>
                        <td align="center">
                            <table style=' width:98%' border='0' id='FiltroTablas'>
                                <tr>
                                    <td align="left" style="width:200px">FECHA INICIO:</td>
                                    <td><input type="text" id="txtfecini"></td>
                                    <td align="left" style="width:200px">FECHA FINAL:</td>
                                    <td><input type="text" id="txtfecfin"></td>
                                </tr>
                                <tr>
                                    <td colspan="8" align="center">
                                        <button id='btnBuscar' class="boton" name='btnBuscar'>
                                            Buscar
                                            <img src="@Url.Content("~/Images/botones/buscar2.png")" width="16px" />
                                        </button>
                                        <button id='btnLimpiar' class="boton" name='btnLimpiar'>
                                            Limpiar
                                            <img src="@Url.Content("~/Images/botones/refresh.png")" width="16px" />
                                        </button>
                                        <div style="float: right;">
                                            <img src="@Url.Content("~/Images/botones/nuevo.png")" alt="Registrar nuevo usuario." title="Registrar nuevo usuario." id="btnNuevo" />&nbsp;
                                            @*<img src="@Url.Content("~/Images/botones/delete.png")" alt="Inactivar datos de usuario." title="Inactivar datos de usuario." id="btnEliminar" />*@
                                            &nbsp;
                                        </div>
                                    </td>
                                </tr>
                                <tr id="trListaTasaCambio">
                                    <td colspan="8">
                                        <div id='divTitulo'>
                                            LISTA - TASAS DE CAMBIO</div>
                                            <div id="grid"></div>
                                    </td>
                                </tr>
                            </table>
                        </td>
                    </tr>

                </table>
            </td>
        </tr>
    </table>

    <div title="Monto de Tasa de Cambio" id="mvAgregarTasaCambio">
        <table width='100%' border='0' id='FiltroTablaPopObs'>
            <input type="hidden" id="hidId" />
            <tr>
                <td>Fecha  - Tipo de Cambio:</td>
                <td>
                    <input type="text" id="txtfecha" readonly>
                </td>
            </tr>
            <tr>
                <td>
                    Valor - Tasa de Cambio :

                </td>
                <td><input type="text" id="txtvalortasacambio"></td>
            </tr>

        </table>
    </div>

</body>
</html>
