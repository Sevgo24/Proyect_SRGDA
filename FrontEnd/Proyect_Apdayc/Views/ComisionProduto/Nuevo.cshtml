@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Comisipón Producto</title>


    <link href="~/Scripts/Extensiones/jquery-ui-1.10.4/css/redmond/jquery-ui-1.10.4.custom.css" rel="stylesheet" />
    <link href="~/Style/SRGDA.css" rel="stylesheet" />

    <link href="@Url.Content("~/Content/styles/kendo.common.min.css")" rel="stylesheet" />
    <script src="@Url.Content("~/Scripts/js/jquery.min.js")"></script>
    <script src="@Url.Content("~/Scripts/js/kendo.web.min.js")"></script>
    <script src="@Url.Content("~/Scripts/Extensiones/jquery-ui-1.10.4/js/jquery-ui-1.10.4.custom.js")"></script>
    <script src="@Url.Content("~/Scripts/SGRDA/comun.dropdownlist.js")"></script>
    <script src="@Url.Content("~/Scripts/SGRDA/Comision/comision.producto.nuevo.js")"></script>
    <script src="@Url.Content("~/Scripts/js/cultures/kendo.culture.es-PE.min.js")"></script>
    <script src="@Url.Content("~/Scripts/SGRDA/generales.js")"></script>
    <script src="@Url.Content("~/Scripts/SGRDA/comun.buscador.modalidaduso.js")"></script>

    <link rel="stylesheet" href="@Url.Content("~/Scripts/Extensiones/alerts/themes/alertify.core.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Scripts/Extensiones/alerts/themes/alertify.default.css")" />
    <script src="@Url.Content("~/Scripts/Extensiones/alerts/lib/alertify.min.js")"></script>

    <style>
        li.ui-state-default.ui-state-hidden[role=tab]:not(.ui-tabs-active) {
            display: none;
        }
    </style>
</head>
<body>
    <div title="Nuevo Agente">
        <div id="ContenedormvBuscarModalidad"></div>
        <table width='100%' border="0" cellspacing='0' cellpadding='0'>
            <tr>
                <td style="vertical-align: bottom">
                    <input id="hidAgeId" type="hidden" />
                    <input id="hidOpcionEdit" type="hidden" />
                    <div id='divTituloPerfil'>Niveles de Agentes Recaudados - Nuevo</div>
                </td>
            </tr>

            <tr>
                <td>
                    <center>

                           <table width='98%' border="0"  id='FiltroTablaPop'>
                                <tr>
                                    <td colspan="2" style="text-align: right;" class="ui-tabs-nav ui-helper-reset ui-helper-clearfix ui-widget-header ui-corner-all">
                                            
                                                        <button id='btnDescartar' class="boton" name='btnDescartar'>
                                                            Volver 
                                                        <img src="@Url.Content("~/Images/botones/back.png")" width="24px" /></button>
                                        &nbsp;
                                                   <button id='btnNuevo' class="boton" name='btnNuevo'>
                                                            Nuevo 
                                                        <img src="@Url.Content("~/Images/botones/neo.png")" width="24px" /></button>
                                        &nbsp;
                                                        <button id='btnGrabar' class="boton" name='btnGrabar'>
                                                            Grabar 
                                                        <img src="@Url.Content("~/Images/botones/save.png")" width="24px" /></button>                                        
                                    
                                    </td>
                                </tr>

                               <tr >

                               </tr>
                                <tr>
                                    <td style="padding-left:20px">Producto : </td>
                                    <td>
                                        <img src="@Url.Content("~/Images/botones/buscar.png")" id="btnBuscarMOD" style="cursor:pointer;" alt="Búsqueda de Modalidad" title="Búsqueda de Modalidad"/>
                                        <input type="hidden" id="hidModalidad"/>
                                        <lable id="lbModalidad" style="cursor:pointer;" alt="Búsqueda de Modalidad" title="Búsqueda de Modalidad">Todos</lable>                                        
                                    </td>
                                </tr>
                               <tr>
                                   <td style="padding-left:20px">Formato Comisión :</td>
                                   <td><select id="ddlFormatoComision" class="requeridoLst"></select></td>
                               </tr>
                               <tr>
                                   <td style="padding-left:20px">Valor</td>
                                   <td><input type="text" id="txtValor" class="requerido" /></td>
                               </tr>
                               <tr>
                                   <td style="padding-left:20px">Nivel Agente :</td>
                                   <td><select id="ddlNivelAgente" class="requeridoLst"></select></td>
                               </tr>
                               <tr>
                                   <td style="padding-left:20px;width:140px">Fecha Vigencia :</td>
                                   <td><input type="text" id="txtFechaVigencia" class="requerido" /></td>
                               </tr>
                            </table>

                    </center>

                </td>
            </tr>

            <tr>
                <td colspan="4" style="text-align: center">
                    <div id='aviso'></div>
                </td>
            </tr>

        </table>
    </div>
</body>
</html>
