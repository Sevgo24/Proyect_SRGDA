@{
    ViewBag.Title = "APDAYC :: COMISIÓN POR TOTAL";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Comisiones por totales - Nuevo</title>

    <script src="@Url.Content("~/Scripts/SGRDA/Comision/comision.total.nuevo.js")"></script>
    <script src="@Url.Content("~/Scripts/SGRDA/generales.js")"></script>
    <script src="@Url.Content("~/Scripts/SGRDA/comun.dropdownlist.js")"></script>
    <script src="@Url.Content("~/Scripts/SGRDA/comun.buscador.socio.js")"></script>
    <script src="@Url.Content("~/Scripts/SGRDA/comun.buscador.socio.aux.js")"></script>
    <script src="@Url.Content("~/Scripts/SGRDA/comun.buscador.tarifa.js")"></script>
    <script src="@Url.Content("~/Scripts/SGRDA/comun.buscador.Licencia.js")"></script>
    <script src="@Url.Content("~/Scripts/SGRDA/comun.buscador.oficina.js")"></script>
    <script src="@Url.Content("~/Scripts/SGRDA/comun.autocompletar.js")"></script>

    <link rel="stylesheet" href="@Url.Content("~/Scripts/Extensiones/alerts/themes/alertify.core.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Scripts/Extensiones/alerts/themes/alertify.default.css")" />
    <script src="@Url.Content("~/Scripts/Extensiones/alerts/lib/alertify.min.js")"></script>

    <style>
        li.ui-state-default.ui-state-hidden[role=tab]:not(.ui-tabs-active) {
            display: none;
        }
    </style>
</head>
<body>
    <div>
        <div id="ContenedormvBuscarSocio"></div>
        <div id="ContenedormvBuscarSocioAux"></div>

        <table width='100%' border='0' cellspacing='0' cellpadding='0'>
            <tr>
                <td style="vertical-align: bottom;">
                    <div id='divTitulo'>Programa de Comisiones por totales - Nuevo</div>
                    <input id="hidOpcionEdit" type="hidden" />
                    <input id="hidAccionMvRep" type="hidden" />
                    <input type="hidden" id="hidIdPrograma" />
                    <input type="hidden" id="hidEdicionRep" />
                    <input type="hidden" id="hidResponsableAux" />
                </td>
            </tr>

            <tr>
                <td>
                    <hr />
                </td>
            </tr>

            <center>
                <table width='98%' border='0'>
                    <tr>
                        <td  colspan="3" style="  text-align:right; "  class="ui-tabs-nav ui-helper-reset ui-helper-clearfix ui-widget-header ui-corner-all">
                            <button id='btnGrabar'  class="boton" name='btnGrabar'>Grabar 
                            <img src="@Url.Content("~/Images/botones/save.png")" width="16px" /></button> 
                            &nbsp;
                            <button id='btnDescartar'  class="boton" name='btnDescartar'>Descartar 
                            <img src="@Url.Content("~/Images/botones/cancelar.png")" width="16px" /></button> 
                        </td>
                    </tr>

                    <tr>
                        <td style="width:15%">Nombre del Programa :</td>
                        <td><input type="text"  id="txtPrograma" maxlength="60" style="width:300px"/></td>
                    </tr>
                    <tr>
                        <td>Fecha último lanzamiento :</td>
                        <td><input type='text' id="txtFechaUltimo"/></td>        
                    </tr>
                    <tr>
                        <td>Temporalidad :</td>
                        <td><select id="ddlTemporalidad"></select></td>           
                    </tr>
                    <tr>
                        <td>Fecha Inicio :</td>
                        <td><input type='text' id="txtFechaInicio"/></td>    
                    </tr>  
                    <tr>   
                        <td>Fecha Fin :</td>
                        <td><input type='text' id="txtFechaFin"/></td>                    
                    </tr>             
                </table>
            </center>

            <tr>
                <td colspan="4" style="height: 10px;"></td>
            </tr>

            <tr>
                <td colspan="2">
                    <div id="tabs">
                        <ul>
                            <li><a href="#tabs-1">Representante</a></li>
                        </ul>
                        <div id="tabs-1">
                            <table style="width: 100%;" border="0">
                                <tr>
                                    <td>
                                        <div>
                                            <img src="~/Images/botones/add.png" class="addRepresentante" height="30">
                                        </div>
                                        <div style="margin-top: -30px; margin-left: 30px;"><a href="#" class="addRepresentante">Agregar Representante  </a></div>
                                        <hr />
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <div id="gridRepresentante"></div>
                                    </td>
                                </tr>
                            </table>
                        </div>
                    </div>
                </td>
            </tr>

        </table>

    </div>

    <div title="Representante" id="mvRepresentante">
        <table width='98%' border="0">
            <tr>
                <td>Representante :</td>
                <td colspan="0">
                    <table border="0" style="border-collapse: collapse;">
                        <tr>
                            <td>
                                <img src="@Url.Content("~/Images/botones/buscar.png")" id="btnBuscarBSaux" style="cursor: pointer;" alt="Búsqueda de Representante" title="Búsqueda de Representante"/>
                            </td>
                            <td colspan="6">
                                <label id="lbResponsableAux" style="cursor: pointer;" alt="Búsqueda de Representante" title="Búsqueda de Modalidad">Seleccione</label>
                            </td>
                        </tr>
                    </table>
                </td>
            </tr>
            <tr>
                <td>Fecha Inicio :</td>
                <td>
                    <input type="text" id="FechaRepIni" /></td>
            </tr>
            <tr>
                <td>Fecha Fin :</td>
                <td>
                    <input type="text" id="FechaRepFin" /></td>
            </tr>
        </table>
    </div>

    <div title="Agregar Rango" id="mvRango">
        <table width="98%" border="0" id="FiltroTablaPopVal">
            <tr>
                <td>
                    <input type="hidden" id="hidAccionMvRan" />
                    <input type="hidden" id="hidEdicionRan" />   
                    <input type="hidden" id="hidEdicionValRan" /> 
                    <input type="hidden" id="hidbpsId" /> 
                    Orden :
                </td>
                <td><input type="text" id="txtOrden" /></td>
            </tr>
            <tr>
                <td>Valor Inicial :</td>
                <td><input type="text" id="txtValorInicial" /></td>
            </tr>
            <tr>
                <td>Valor Final :</td>
                <td><input type="text" id="txtValorFinal" /></td>
            </tr>
            <tr>
                <td>Formato Comisión :</td>
                <td><select id="ddlFormatoComision"> </select></td>
            </tr>
            <tr>
                <td>Valor Comisión :</td>
                <td>
                    <input type="text" id="txtValorComision" />
                </td>
               
            </tr>
        </table>
    </div>

</body>
</html>
