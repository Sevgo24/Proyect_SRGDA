@{
    ViewBag.Title = "Consulta Establecimiento";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Consulta Establecimiento</title>
    <link href="@Url.Content("~/Content/styles/kendo.common.min.css")" rel="stylesheet" />
    <script src="@Url.Content("~/Scripts/js/jquery.min.js")"></script>
    <script src="@Url.Content("~/Scripts/js/kendo.web.min.js")"></script>
    <script src="@Url.Content("~/Scripts/Extensiones/jquery-ui-1.10.4/js/jquery-ui-1.10.4.custom.js")"></script>

    <script src="@Url.Content("~/Scripts/SGRDA/Consulta/consulta.establecimiento.js")"></script>
    <script src="@Url.Content("~/Scripts/SGRDA/comun.buscador.socio.js")"></script>
    <script src="@Url.Content("~/Scripts/SGRDA/generales.js")"></script>
    <script src="@Url.Content("~/Scripts/SGRDA/comun.dropdownlist.js")"></script>
    <script src="@Url.Content("~/Scripts/SGRDA/comun.autocompletar.js")"></script>
    <script src="@Url.Content("~/Scripts/SGRDA/comun.autocompletar.js")"></script>
    <script src="@Url.Content("~/Scripts/SGRDA/popup.direccion.js")"></script>
</head>
<body>
    <div id="ContenedormvBuscarSocio"></div>

    <center>
        @*<table border="0" width="98%" id ="FiltroTabla">*@       
        <table border="0" width="98%" >       
            <tr>
                <td>
                     <div id="divTitulo">Consulta Establecimiento</div>
                      <hr />
                </td>
            </tr>   
            <tr>
                <td>
                    <table style="width:100%">
                        <tr>
                            <td>
                                Id Establecimiento :
                            </td>
                            <td>
                                <input type="text" id="txtId" style="width:70px" />
                            </td>
                        </tr>
                        <tr>
                            <td>
                                Nombre :
                            </td>
                            <td>
                                <input type="text" id="txtNombre" style="width:250px" />
                            </td>
                        </tr>
                        <tr>
                            <td>
                                Usuario de Derecho :
                            </td>
                            <td>
                                <table border="0" style="border-collapse: collapse;" title="Usuario de Derecho">
                                    <tr>
                                        <td>
                                            <img src="@Url.Content("~/Images/botones/buscar.png")" id="btnBuscarBS" style="cursor: pointer;" alt="Búsqueda de Socio" title="Búsqueda de Socio"/>
                                        </td>
                                        <td colspan="6">
                                            <label id="lbResponsable" style="cursor: pointer;" alt="Búsqueda de Socio" title="Búsqueda de Socio"> Seleccione un usuario de derecho.</label>                                
                                        </td>
                                        <td>
                                            <input type="hidden" id="hidAccionMvEnt" />
                                            <input type="hidden" id="hidEdicionEnt" value="0"/>
                                        </td>
                                    </tr>
                                </table>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                Tipo Establecimiento :
                            </td>
                            <td>
                                <select id="ddlTipoEstablecimiento"></select>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                SubTipo Establecimiento :
                            </td>
                            <td>
                                <select id="ddlSubtipoestablecimiento"></select>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                División Administrativa :
                            </td>
                            <td>
                                <select id="ddlDivisionAdm"></select>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                Territorio :
                            </td>
                            <td>
                                <select id="ddlTerritorio"></select>
                            </td>
                        </tr>
                        <tr>
                            <td style="width:160px">
                                Ubigeo :
                            </td>
                            <td>
                                  <input type="text" id="txtUbigeo"  size="40" style="width:400px"/>
                                  <input type="hidden"   id="hidCodigoUbigeo" />
                            </td>
                            <td>
                                 <div style="text-align:right">
                                    <button id="btnLimpiar" name="btnLimpiar">
                                        Limpiar
                                        <img src="@Url.Content("~/Images/botones/refresh.png")" width="16px" />
                                    </button>
                                    &nbsp&nbsp&nbsp
                                    <button id="btnBuscar" name="btnBuscar">
                                        Buscar
                                        <img src="@Url.Content("~/Images/botones/buscar2.png")" width="16px" />
                                    </button>
                                </div>
                            </td>
                        </tr>
                    </table>
                </td>
            </tr>              
            <tr>
                <td>
                    <div id="grid"></div>
                </td>
            </tr>
        </table>
    </center>


</body>
</html>
