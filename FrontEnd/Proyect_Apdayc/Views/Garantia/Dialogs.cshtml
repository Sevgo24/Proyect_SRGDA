
<div title="Agregar Garantia" id="mvGarantia">
    <table width='100%' border='0' id='FiltroTablaPop'>
         <tr>
            <td> Tipo :</td>
            <td>
                <select id="ddlTipoGar" class="requeridoLst">
                    <option value="0">--SELECCIONE--</option>
                    <option value="P">POLIZA</option>
                    <option value="S">SEGURO</option>
                </select>
            </td>
            <td>Numero :</td>
            <td>
                <input type="text" id="txtNumeroGar"  maxlength="50"/>
            </td>
        </tr>
         <tr>
            <td>Entidad</td>
            <td colspan="3">
                 <input type="text" id="txtEntidad" maxlength="50" class="requerido" style=" width:300px;"/>
            </td>
        </tr>
        <tr>
            <td> Valor:</td>
            <td>
               <input type="hidden" id="hidAccionMvGar" />
               <input type="hidden" id="hidEdicionGar" /> 
               
                <input type="text" id="txtValorGar" class="requerido" maxlength="6"/>
            </td>
            <td>Moneda:</td>
            <td>
                <select id="ddlMonedaGar"  class="requeridoLst" />                  
            </td>
        </tr>
       
       

      <tr>
          <td>Fecha de Recepcion</td>
          <td colspan="1">
              <input type="text" id="txtFechaReceepcion" class="k-fecha requerido" />
          </td>
         <td><label id="lblFechaDevolucion">Fecha de Devolución</label></td>
          <td>
              <input type="text" id="txtFechaDevolucion"  class="k-fecha"/>
          </td> 
      </tr>
