@model SGRDA.Entities.BEOfficeMin
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Reporte Arbol de Oficinas de Recaudo</title>


    <link href="@Url.Content("~/Content/styles/kendo.common.min.css")" rel="stylesheet" />
    <script src="@Url.Content("~/Scripts/js/jquery.min.js")"></script>
    <script src="@Url.Content("~/Scripts/js/kendo.web.min.js")"></script>
    <script src="@Url.Content("~/Scripts/Extensiones/jquery-ui-1.10.4/js/jquery-ui-1.10.4.custom.js")"></script>
    <script src="@Url.Content("~/Scripts/SGRDA/Mantenimientos/mantenimiento.office.treeview.js")"></script>
    <script src="@Url.Content("~/Scripts/SGRDA/generales.js")"></script>
    <link href="~/Scripts/Extensiones/jquery-ui-1.10.4/css/redmond/jquery-ui-1.10.4.custom.css" rel="stylesheet" />
    <link href="~/Style/SRGDA.css" rel="stylesheet" />


    <script src="~/Scripts/jquery.jstree.js"></script>


</head>
<body>


    <div id="divTree">
        <center>
            <table style="width: 95%" >

            <tr>
                <td style="text-align: center">
                    <img src="~/Images/botones/apdayLogo.jpg" width="70px" height="90px" />
                </td>
                <td style="text-align: center">
                    <div ><h2>Reporte Árbol de Oficinas de Recaudo</h2></div>
                </td>
                <td>
                    <div><h4> @ViewBag.Fecha</h4></div>
                    <div><h4>@ViewBag.Hora</h4></div>
                    <div><h4>@ViewBag.Usuario</h4></div>
                    
                </td>
                
            </tr>           
           
            <tr>
                <td colspan="3">
                    <hr noshade="noshade" />
                </td>
            </tr>

              <tr>
                  <td colspan="2">
                      <div id="treeview"></div>
                  </td>
                  <td style="vertical-align:top; text-align:left">
                                   <button id='btnImprimir'  class="boton" name='btnImprimir'>Imprimir 
                                        <img src="@Url.Content("~/Images/botones/print.png")" width="16px" />
                                   </button>                        
                  </td>
              </tr>      

        </table>
        </center>
    </div>





</body>
</html>
