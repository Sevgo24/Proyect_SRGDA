@{
    ViewBag.Title = "APDAYC :: Proceso";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Procesos</title>


    <link href="~/Scripts/Extensiones/jquery-ui-1.10.4/css/redmond/jquery-ui-1.10.4.custom.css" rel="stylesheet" />
    <link href="~/Style/SRGDA.css" rel="stylesheet" />

    <link href="@Url.Content("~/Content/styles/kendo.common.min.css")" rel="stylesheet" />
    <script src="@Url.Content("~/Scripts/js/jquery.min.js")"></script>
    <script src="@Url.Content("~/Scripts/js/kendo.web.min.js")"></script>
    <script src="@Url.Content("~/Scripts/Extensiones/jquery-ui-1.10.4/js/jquery-ui-1.10.4.custom.js")"></script>
    <script src="@Url.Content("~/Scripts/SGRDA/comun.dropdownlist.js")"></script>
    <script src="@Url.Content("~/Scripts/SGRDA/Mantenimientos/mantenimiento.procesos.nuevo.js")"></script>
    <script src="@Url.Content("~/Scripts/SGRDA/generales.js")"></script>

    <link rel="stylesheet" href="@Url.Content("~/Scripts/Extensiones/alerts/themes/alertify.core.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Scripts/Extensiones/alerts/themes/alertify.default.css")" />
    <script src="@Url.Content("~/Scripts/Extensiones/alerts/lib/alertify.min.js")"></script>

    <style>
        li.ui-state-default.ui-state-hidden[role=tab]:not(.ui-tabs-active) {
            display: none;
        }
    </style>

</head>
<body>
    <div title="Nuevo Proceso">

        <table width='100%' border="0" cellspacing='0' cellpadding='0'>
            <tr>
                <td style="vertical-align: bottom">
                    <input type="hidden" id="hidCodigoPRO" />
                    <input id="hidOpcionEdit" type="hidden" />
                </td>
            </tr>

            <tr>
                <td>
                    <center>
                           <table width='98%' border="0"  id='FiltroTablaPop'>
                                <tr>
                                    <td colspan="4" style="text-align: right;" class="ui-tabs-nav ui-helper-reset ui-helper-clearfix ui-widget-header ui-corner-all">
                                         <table  width='100%' border='0' id='FiltroTabla'>
                                           <td style="width:500px;text-align:left;">
                                               <div id="divTituloPerfil">Proceso / Nuevo</div>
			                               </td>
                                            <td colspan="1">
                                                <button id='btnDescartar' class="boton" name='btnDescartar'> Volver 
                                                <img src="@Url.Content("~/Images/botones/back.png")" width="24px" /></button>&nbsp;
                                                <button id='btnGrabar' class="boton" name='btnGrabar'>Grabar 
                                                <img src="@Url.Content("~/Images/botones/save.png")" width="24px" /></button>          
                                            </td>
                                         </table>
                                    </td>
                                </tr>
                                <tr >
                                    <td></td>        
                                </tr>

                                <tr>
                                    <td>
                                        <table border="0">
                                            <tr>
                                                <td style="padding-left:30px;width:140px">
                                                    <label id="codigo">Id Tipo de Proceso :</label>
                                                </td>
                                                <td style="width:150px">
                                                    <input type="text" id="txtCodigo" style="width:120px" maxlength="3"/>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td style="padding-left:30px;width:90px">
                                                    Proceso :
                                                </td>
                                                <td>
                                                    <input type="text" id="txtNombre"  style="width:400px" maxlength="60" class="requerido" />
                                                </td>
                                                <td>
                                                    <input type="checkbox" id="chkVisible" />
                                                    Mostrar al usuario
                                                </td>
                                            </tr>
                                             <tr>
                                                <td style="padding-left:30px;width:90px">
                                                    Descipción  <br />
                                                    del Proceso :
                                                </td>
                                                <td>                                                    
                                                    <textarea id="txtDescripcion" maxlength="255"  rows="2" cols="60"></textarea>
                                                </td>
                                            </tr>
                                       
                                            <tr>
                                                <td style="padding-left:30px;width:90px">
                                                    Tipo de proceso :
                                                </td>
                                                <td> 
                                                    <select id="ddlTipoproceso" class="requeridoLst"></select>
                                                </td>
                                            </tr>
                                              
                                              <tr>
                                                <td style="padding-left:30px;width:90px">
                                                    Workflow :
                                                </td>
                                                <td> 
                                                    <select id="ddlCiclo" class="requeridoLst"></select>
                                                </td>
                                            </tr>
                                              <tr>
                                                <td style="padding-left:30px;width:90px">
                                                    Módulo :
                                                </td>
                                                <td> 
                                                    <select id="ddlCliente" class="requeridoLst"></select>
                                                </td>
                                            </tr>
                                              <tr>
                                                <td style="padding-left:30px;width:90px">
                                                    Función Principal :
                                                </td>
                                                <td>                                                    
                                                    <textarea id="txtFunción" maxlength="255" class="requerido" rows="2" cols="60"></textarea>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td style="padding-left:30px;width:115px">
                                                                    Nro Veces a Ejecución :
                                                                </td>
                                                <td colspan="4">
                                                    <table >

                                                        <tr>
                                                              
                                                                <td style="width:150px">
                                                                    <input type="text" id="txtNroEjecutar" style="width:120px" maxlength="3" class="requerido"/>
                                                                </td>

                                                                <td style="padding-left:100px">Tipo de Frecuencia :</td>
                                                                <td> 
                                                                    <select id="ddlFrecuenia" class="requeridoLst"></select>
                                                                </td>
                                                        </tr>
                                                    </table>
                                                </td>

                                            </tr>
                                            <tr>
                                                <td style="padding-left:30px;width:115px">
                                                    Fecha Ultimo de Lanzamiento :
                                                </td>
                                                <td style="width:150px">
                                                    <input type="text" id="txtFecha" style="width:120px" class="requerido"/>
                                                </td>
                                            </tr>
                                        </table>
                                </td>    
                                </tr>
                            </table>

                    </center>

                </td>
            </tr>

            <tr>
                <td colspan="4" style="text-align: center">
                    <div id='aviso'></div>
                </td>
            </tr>

        </table>
    </div>
</body>
</html>
