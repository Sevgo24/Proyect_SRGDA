    <script src="@Url.Content("~/Scripts/Extensiones/alerts/lib/alertify.min.js")"></script>

    <style>
        li.ui-state-default.ui-state-hidden[role=tab]:not(.ui-tabs-active) {
            display: none;
        }
    </style>
</head>
<body>
    <div title="Registrar Modalidad de Sociedad">

        <table style="width: 100%" border="0">
            <tr>
                <td>
                    <input id="hidOpcionEdit" type="hidden" />
                    <input id="hidId" type="hidden" />
                    
                </td>
            </tr>


            <tr>
                <td>
                    <center>
                        <table style="width:98%" border="0" id="FiltroTablaPop">
                            <tr>
                                <td colspan="4" style="text-align: right;" class="ui-tabs-nav ui-helper-reset ui-helper-clearfix ui-widget-header ui-corner-all">
                                           
                                    
                                    <table  width='100%' border='0' id='FiltroTabla'>
		   <tr>
			   <td style="width:500px;text-align:left;">
				   <div id="divTituloPerfil">Modalidad de Sociedad - Nuevo</div>
			   </td>
				<td  colspan="1">
					<button id='btnDescartar' class="boton" name='btnDescartar'>
                                        Volver 
                                        <img src="@Url.Content("~/Images/botones/back.png")" width="24px" /></button>
                                &nbsp;
                                        <button id='btnNuevo' class="boton" name='btnNuevo'>
                                        Nuevo 
                                        <img src="@Url.Content("~/Images/botones/neo.png")" width="24px" /></button>
                                &nbsp;
                                        <button id='btnGrabar' class="boton" name='btnGrabar'>
                                        Grabar 
                                        <img src="@Url.Content("~/Images/botones/save.png")" width="24px" /></button>
					
				</td>   
		   </tr>
	   </table>                     
                                        
                                </td>
                            </tr>
                            <tr>
                                <td></td>
                            </tr>
                            <tr>
                                <td>
                                        <table>
                                              <tr>
                                                <td style="padding-left:30px;width:90px">
                                                    Código :
                                                </td>
                                                <td style="width:150px">
                                                    <input type="text" id="txtCodigo"  style="width:120px" maxlength="3" placeholder="Ingrese Código" class="requerido"/>
                                                </td>
                                                  <td>
                                                      <div id="divResultValidacionCod"></div>
                                                  </td>
                                            </tr>
                                            <tr>
                                                <td style="padding-left:30px;width:90px">
                                                    Sociedad :
                                                </td>
                                                <td>
                                                    <input type="text" id="txtSociedad"  style="width:300px" maxlength="40" placeholder="Ingrese Sociedad" class="requerido" />
                                                </td>
                                                <td>
                                                    <div id="divResultValidacionDes"></div>
                                                </td>

                                            </tr>
                                        </table>
                                </td>
                            </tr>
                            <tr>
                                <td></td>
                            </tr>
                        </table>
                    </center>
                </td>
            </tr>


        </table>
         <center> <div id="aviso"></div> </center>
    </div>
