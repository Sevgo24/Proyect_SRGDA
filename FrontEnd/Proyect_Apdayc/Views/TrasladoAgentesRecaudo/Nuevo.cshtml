@{
    ViewBag.Title = "APDAYC :: TRASLADO AGENTE RECAUDO";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Traslado de agentes</title>
    <link href="~/Scripts/Extensiones/jquery-ui-1.10.4/css/redmond/jquery-ui-1.10.4.custom.css" rel="stylesheet" />
    <link href="~/Style/SRGDA.css" rel="stylesheet" />

    <link href="@Url.Content("~/Content/styles/kendo.common.min.css")" rel="stylesheet" />
    <script src="@Url.Content("~/Scripts/js/jquery.min.js")"></script>
    <script src="@Url.Content("~/Scripts/js/kendo.web.min.js")"></script>
    <script src="@Url.Content("~/Scripts/Extensiones/jquery-ui-1.10.4/js/jquery-ui-1.10.4.custom.js")"></script>
    <script src="@Url.Content("~/Scripts/SGRDA/Mantenimientos/mantenimiento.trasladoagente.nuevo.js")"></script>
    <script src="@Url.Content("~/Scripts/SGRDA/generales.js")"></script>
    <script src="@Url.Content("~/Scripts/SGRDA/comun.dropdownlist.js")"></script>
    <script src="@Url.Content("~/Scripts/SGRDA/comun.autocompletar.js")"></script>

    <link rel="stylesheet" href="@Url.Content("~/Scripts/Extensiones/alerts/themes/alertify.core.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Scripts/Extensiones/alerts/themes/alertify.default.css")" />
    <script src="@Url.Content("~/Scripts/Extensiones/alerts/lib/alertify.min.js")"></script>

    <style>
        li.ui-state-default.ui-state-hidden[role=tab]:not(.ui-tabs-active) {
            display: none;
        }
    </style>

    <style>
        .ui-autocomplete {
            max-height: 200px;
            overflow-y: auto;
            overflow-x: hidden;
        }

        * html .ui-autocomplete {
            height: 200px;
        }

        ul.ui-autocomplete {
            z-index: 1100;
        }
    </style>
</head>
<body>
    <div>
        <table width='100%' border='0' cellspacing='0'>
            <tr>
                <td style="vertical-align: bottom;">
                    @*<div id="divTitulo">Nuevo traslado de Agente</div>*@

                    <input type="hidden" id="hidCodigoOFI">
                    <input type="hidden" id="hidCodigoBPS">
                </td>
            </tr>
            <tr>
                <td style="vertical-align: top;">
                    <hr>
                </td>
            </tr>
            <tr>
                <td>
                    <center>
                    <table width='98%' border='0' id='FiltroTabla'>
                        <tr>
                              <td  colspan="3" style="  text-align:right; "  class="ui-tabs-nav ui-helper-reset ui-helper-clearfix ui-widget-header ui-corner-all">
                                  <table border="0" style=" width:100%;">
                        <tr>
                            <td>
                                <div id='divTituloPerfil' style="  text-align:left">
                                 Traslado de Agente - Nuevo
                                     </div>

                            </td>
                            <td>
                    
                                <div style="float:right;">

                                    <button id='btnDescartar'  class="boton" name='btnDescartar'>Volver 
                                    <img src="@Url.Content("~/Images/botones/back.png")" width="24px" /></button> 
                                
                                    &nbsp;
                                    <button id='btnGrabar'  class="boton" name='btnGrabar'>Grabar 
                                    <img src="@Url.Content("~/Images/botones/save.png")" width="24px" /></button> 
                              </div>   

                                </td>
                </tr>
                </table>
                        </tr>
                        <tr>
                            <td>
                                <table border="0">
                                    <tr>
                                         <td>Oficina actual</td>
                                         <td> <input type="text" id="txtOficinaActual" style="width: 240px"/></td> 
                                         <td>  </td>  
                                    </tr>

                                    <tr>                                       
                                         <td>Oficina destino</td>
                                         <td><select id="ddlOficinaDestino" style="width: 250px" class="requeridoLst"></select></td>
	                                     <td>  </td>                                           
                                    </tr>

                                    <tr>                                       
                                         <td>Nivel de agente</td>
                                         <td><select id="ddlNivelAgente" style="width: 250px" class="requeridoLst"></select></td>
	                                     <td>  </td>                                           
                                    </tr>

                                    <tr>
                                         <td>Fecha inicio</td>
                                         <td> <input type="text" id="txtFechaInicio" style="width: 130px"/></td>
                                         <td>  </td>  
                                    </tr>

                                    <tr>
                                         <td>Fecha fin</td>
                                         <td> <input type="text" id="txtFechaFin" style="width: 130px"/></td>
                                         <td>  </td>  
                                    </tr>
                                </table>
                            </td>
                        </tr>
                          @*<table  width='98%' border='0' id='FiltroTabla'>
                               <tr>
                                     <td>                                         
                                            <div id="gridAgenteRecaudo"></div>
                                     </td>
                                 </tr>
                          </table>*@
                    </table>
                    </center>
                </td>
            </tr>
        </table>
    </div>
</body>
</html>
